// Task 1

function sum (a, b) {
  return a + b;
}

// Task 2

function convert (minutesToSeconds) {
  return minutes * 60;
}

// Task 3

function convertYearsToDays (age) {
  return age * 365;
}

// Task 4

function nameString (name) {
  let result = name + 'Edabit';
  return result;
}

// Task 5

function equalNumbers (num1, num2) {
  return num1 === num2;
}

